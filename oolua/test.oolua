class MyException(Exception)
end

try
    error(MyException("Test"))
except Exception as e
    print(e)
except MyException as e
    print("MyException occured")
    print(e)
end

print(MyException:is(Exception)) -- true

class AnotherException(Exception)
end

print(MyException:is(AnotherException)) -- false

print(MyException:is(MyException)) -- true

class MoreException(AnotherException)
end

print(MoreException:is(AnotherException)) -- true
print(MoreException:is(Exception)) -- true

class A(Exception) end
